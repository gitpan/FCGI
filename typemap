TYPEMAP
FCGI	T_FCGIOBJNOMUNGE
FCGI::Stream T_FCGISTREAM

INPUT
T_FCGIOBJNOMUNGE
    if (sv_isa($arg, \"FCGI\")) {
	$var = ($type) SvIV((SV*)SvRV($arg));
    } else
	croak(\"$var is not of type ${ntype}\")
T_FCGISTREAM
    if (sv_isa($arg, \"FCGI::Stream\")) {
	$var = ($type) SvIV((SV*)SvRV($arg));
    } else
	croak(\"$var is not of type ${ntype}\")
